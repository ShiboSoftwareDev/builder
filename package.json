{
  "name": "@tscircuit/builder",
  "version": "1.2.3",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "test": "ava",
    "build": "tsup ./src --sourcemap --dts",
    "build:docs": "typedoc",
    "typecheck": "tsc --noEmit",
    "check:circular": "dpdm --skip-dynamic-imports circular --no-warning --no-tree -T --exit-code circular:1 ./src/index.ts",
    "yalc": "npm run build && yalc publish --push"
  },
  "devDependencies": {
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^9.0.1",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "ava": "^4.3.3",
    "dpdm": "^3.13.0",
    "esbuild": "^0.15.18",
    "esbuild-register": "^3.3.3",
    "esbuild-runner": "^2.2.1",
    "prettier": "^2.7.1",
    "redaxios": "^0.5.1",
    "tsup": "^6.2.3",
    "type-fest": "^2.19.0",
    "typedoc": "^0.23.15",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "@lume/kiwi": "^0.1.0",
    "@tscircuit/eagle-xml-converter": "^0.0.7",
    "@tscircuit/routing": "^1.0.2",
    "@tscircuit/sparkfun-packages": "^1.0.3",
    "convert-units": "^2.3.4",
    "minisearch": "^6.1.0",
    "parsel-js": "^1.0.2",
    "rectilinear-router": "^1.0.1",
    "svg-path-bounds": "^1.0.2",
    "transformation-matrix": "^2.12.0"
  }
}
